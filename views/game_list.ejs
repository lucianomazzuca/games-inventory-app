<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/game_list.css' />
  </head>
  <body>

    <%- include('partials/nav'); %>   

    
    <h1><%= title %></h1>
    
    <ul>
        <% games.forEach(game => { %>
            <li><a href="<%= game.url %> "><%= game.title %></a></li>
        <% }) %>
    </ul>

    <a href="/games/add">Add New</a>
  </body>
</html>
